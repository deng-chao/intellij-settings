<keymap version="1" name="WindowsOnMac" parent="$default">
  <action id="$Copy">
    <keyboard-shortcut first-keystroke="ctrl c" />
    <keyboard-shortcut first-keystroke="meta c" />
  </action>
  <action id="$Paste">
    <keyboard-shortcut first-keystroke="ctrl v" />
    <keyboard-shortcut first-keystroke="meta v" />
  </action>
  <action id="CloseProject">
    <keyboard-shortcut first-keystroke="shift alt f4" />
  </action>
  <action id="Editor Copy">
    <keyboard-shortcut first-keystroke="meta c" />
  </action>
  <action id="Editor Paste">
    <keyboard-shortcut first-keystroke="meta v" />
  </action>
  <action id="Generate">
    <keyboard-shortcut first-keystroke="alt help" />
  </action>
  <action id="MaximizeActiveDialog">
    <keyboard-shortcut first-keystroke="shift ctrl f11" />
  </action>
  <action id="NewElementInMenuGroup">
    <keyboard-shortcut first-keystroke="alt help" />
  </action>
</keymap>