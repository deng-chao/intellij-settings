<templateSet group="lua">
  <template name="req" value="local $END$ = require &quot;$TABLE$&quot;" description="require" toReformat="false" toShortenFQNames="true">
    <variable name="TABLE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="LUA" value="true" />
    </context>
  </template>
  <template name="loge" value="ngx.log(ngx.ERR, &quot;$INFO$&quot;)" description="ngx.log(ngx.ERR, &quot;&quot;)" toReformat="false" toShortenFQNames="true">
    <variable name="INFO" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="LUA" value="true" />
    </context>
  </template>
  <template name="logi" value="ngx.log(ngx.INFO, &quot;$MSG$&quot;)" description="ngx.log(ngx.INFO, msg)" toReformat="false" toShortenFQNames="true">
    <variable name="MSG" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="LUA" value="true" />
    </context>
  </template>
</templateSet>